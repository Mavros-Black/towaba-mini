# Vercel ignore file
# These files should not be deployed

# Environment files
.env.local
.env.development
.env.test

# Temporary scripts
scripts/fix-*.js
scripts/test-*.js
scripts/debug-*.js

# Documentation that might contain examples
docs/
*.md

# Test files
**/*.test.js
**/*.test.ts
**/*.spec.js
**/*.spec.ts

# Logs
*.log
logs/

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/

# nyc test coverage
.nyc_output

# Dependency directories
node_modules/

# Optional npm cache directory
.npm

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variables file
.env

# next.js build output
.next

# Nuxt.js build output
.nuxt

# Gatsby files
.cache/
public

# Storybook build outputs
.out
.storybook-out

# Temporary folders
tmp/
temp/